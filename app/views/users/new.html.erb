<h1>New Member</h1>

<% if @user.member_detail == nil %>
	<% @user.build_member_detail %>
<% end %>
<%= form_for(@user, :url => users_admin_index_url(@user, quickform: params[:quickform] == "true"), :method => :post) do |form| %>
